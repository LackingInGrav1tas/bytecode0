set num = @input("to what number would you like to count? ") as NUM;

set mut i: NUM = 0;
while ($i < $num) {
    i += 1;

    if ($i mod 15 == 0)      print "FizzBuzz";
    else if ($i mod 3 == 0)  print "Fizz";
    else if ($i mod 5 == 0)  print "Buzz";
    else                     print $i;
    
    print " ";
}